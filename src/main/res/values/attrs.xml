<?xml version="1.0" encoding="utf-8"?>

<resources>

	<declare-styleable name="ColorPickerPreference">

		<attr format="reference"
			  name="colors" />

		<attr format="reference"
			  name="labels" />

	</declare-styleable>

	<declare-styleable name="FontPickerPreference">

		<attr format="string"
			  name="preview" />

	</declare-styleable>

	<declare-styleable name="View.Pager">

		<attr format="boolean"
			  name="swipe" />

	</declare-styleable>

	<declare-styleable name="Widget.Validation">

		<attr format="reference"
			  name="icon" />

		<attr format="reference"
			  name="matches" />

		<attr format="string"
			  name="message" />

		<attr format="string"
			  name="regex" />

		<attr format="boolean"
			  name="required" />

		<attr name="rule">

			<enum name="alpha"
				  value="1" />

			<enum name="alphaDash"
				  value="2" />

			<enum name="alphaNumeric"
				  value="3" />

			<enum name="email"
				  value="4" />

			<enum name="integer"
				  value="5" />

			<enum name="numeric"
				  value="6" />

			<enum name="phone"
				  value="7" />

		</attr>

	</declare-styleable>

</resources>