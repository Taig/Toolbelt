<?xml version="1.0" encoding="utf-8"?>

<resources>

	<declare-styleable name="ColorPickerPreference">

		<attr format="reference"
			  name="colors" />

		<attr format="reference"
			  name="labels" />

	</declare-styleable>

	<declare-styleable name="FontPickerPreference">

		<attr format="string"
			  name="preview" />

	</declare-styleable>

	<attr name="timePickerStyle"
		  format="reference" />

	<declare-styleable name="View.Pager">

		<attr format="boolean"
			  name="swipe" />

	</declare-styleable>

	<declare-styleable name="Widget.Image">

		<attr name="android:src" />

	</declare-styleable>

	<declare-styleable name="Widget.Image.AspectRatio">

		<attr name="dominance">

			<enum name="auto"
				  value="1" />

			<enum name="width"
				  value="1" />

			<enum name="height"
				  value="0" />

		</attr>

		<attr format="float|boolean"
			  name="ratio" />

	</declare-styleable>

	<declare-styleable name="Widget.Image.Radius">

		<attr format="dimension|boolean"
			  name="radius" />

	</declare-styleable>

	<declare-styleable name="Widget.Text.HintStyle">

		<attr name="hintTextStyle">

			<flag name="normal"
				  value="0" />

			<flag name="bold"
				  value="1" />

			<flag name="italic"
				  value="2" />

		</attr>

	</declare-styleable>

	<declare-styleable name="Widget.Validation">

		<attr format="boolean"
			  name="alpha" />

		<attr format="string"
			  name="alphaMessage" />

		<attr format="boolean|string"
			  name="alphaDash" />

		<attr format="string"
			  name="alphaDashMessage" />

		<attr format="boolean|string"
			  name="alphaNumeric" />

		<attr format="string"
			  name="alphaNumericMessage" />

		<attr format="boolean|string"
			  name="email" />

		<attr format="string"
			  name="emailMessage" />

		<attr name="icon" />

		<attr format="boolean|string"
			  name="integer" />

		<attr format="string"
			  name="integerMessage" />

		<attr format="integer"
			  name="length" />

		<attr format="string"
			  name="lengthMessage" />

		<attr format="reference"
			  name="matches" />

		<attr format="string"
			  name="matchesMessage" />

		<attr format="integer"
			  name="max" />

		<attr format="string"
			  name="maxMessage" />

		<attr format="integer"
			  name="min" />

		<attr format="string"
			  name="minMessage" />

		<attr format="boolean|string"
			  name="numeric" />

		<attr format="string"
			  name="numericMessage" />

		<attr format="boolean|string"
			  name="regex" />

		<attr format="string"
			  name="regexMessage" />

		<attr format="string"
			  name="pattern" />

		<attr format="boolean"
			  name="required" />

		<attr format="string"
			  name="requiredMessage" />

		<attr format="boolean|string"
			  name="phone" />

		<attr format="string"
			  name="phoneMessage" />

	</declare-styleable>

</resources>