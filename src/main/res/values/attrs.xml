<?xml version="1.0" encoding="utf-8"?>

<resources>

	<declare-styleable name="ColorPickerPreference">

		<attr format="reference"
			  name="colors" />

		<attr format="reference"
			  name="labels" />

	</declare-styleable>

	<declare-styleable name="FontPickerPreference">

		<attr format="string"
			  name="preview" />

	</declare-styleable>

	<declare-styleable name="View.Pager">

		<attr format="boolean"
			  name="swipe" />

	</declare-styleable>

	<declare-styleable name="Widget.Validation">

		<attr format="string"
			  name="error" />

		<attr format="reference"
			  name="matches" />

		<attr format="reference|string"
			  name="regex" />

		<attr format="flag"
			  name="rules">

			<flag name="alpha"
				  value="0x001" />

			<flag name="alphaDash"
				  value="0x002" />

			<flag name="alphaNumeric"
				  value="0x004" />

			<flag name="email"
				  value="0x008" />

			<flag name="integer"
				  value="0x016" />

			<flag name="numeric"
				  value="0x032" />

			<flag name="phone"
				  value="0x064" />

			<flag name="required"
				  value="0x128" />

		</attr>

	</declare-styleable>

</resources>